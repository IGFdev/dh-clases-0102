<!DOCTYPE html>
<html lang="en">
 
<head>
    <%- include('../partials/head') %>
</head>

<body>

    <% productos.forEach(producto => { %>
        <article>
            <img src="<%= producto.img %>" alt="">
            <h1><%= producto.name %></h1>
            <h1><%= producto.icon %></h1>
            <p><%= producto.price %></p>
            <br>
        </article>
    <% }) %>

    <% %>
    <%= %>

    <ul>
        <% productos.forEach(producto => { %>
            <li><%= producto.id %></li>
        <% }) %>
    </ul>

</body>
</html>